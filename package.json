{
  "name": "plate-scraper",
  "version": "1.0.0",
  "description": "An app for collecting, editing, organizing, and storing recipes",
  "author": "spencer",
  "license": "Unlicense",
  "main": "server/index",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "clean": "rm -rf built dist node_modules && npm install",
    "seed": "NODE_ENV='script' npx ts-node server/scripts/seed.ts",
    "cleanupOrphanedTags": "NODE_ENV='script' npx ts-node server/scripts/cleanupOrphanedTags.ts",
    "build:be": "tsc",
    "build:fe": "webpack --mode=production",
    "build": "npm run build:be & npm run build:fe",
    "start": "node built",
    "dev:be": "ts-node-dev server",
    "dev:fe": "node built & webpack serve --mode=development",
    "dev": "ts-node-dev server & webpack serve --mode=development",
    "test:be": "NODE_ENV='test' mocha -r ts-node/register --file server/mochaSetup.ts --timeout 10000 --exit -- 'server/**/__tests__/*.spec.ts'",
    "heroku-postbuild": "webpack  --mode=production"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "**.js": [
      "eslint --fix --max-warnings=0"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@hot-loader/react-dom": "^16.14.0",
    "@types/cheerio": "^0.22.18",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/node": "^13.13.9",
    "@types/passport": "^1.0.3",
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "class-validator": "^0.12.2",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "connect-typeorm": "^1.1.3",
    "core-js": "^3.8.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "focus-trap-react": "^6.0.0",
    "helmet": "^4.2.0",
    "normalize.css": "^8.0.1",
    "passport": "^0.4.1",
    "pg": "^8.2.1",
    "qs": "^6.9.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^5.7.2",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "@types/supertest": "^2.0.9",
    "babel-loader": "^8.2.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.2.0",
    "eslint-config-swhitehead": "^1.0.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "mini-css-extract-plugin": "^1.3.3",
    "mocha": "^7.2.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.1.1",
    "volleyball": "^1.5.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
