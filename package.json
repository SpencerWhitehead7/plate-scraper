{
  "name": "plate-scraper",
  "version": "1.0.0",
  "description": "An app for collecting, editing, organizing, and storing recipes",
  "author": "spencer",
  "license": "Unlicense",
  "main": "server/index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpencerWhitehead7/plate-scraper.git"
  },
  "scripts": {
    "lint": "eslint ./client",
    "lint:fix": "eslint ./client --fix",
    "clean": "rm -rf built_client built_server node_modules && npm install",
    "seed": "NODE_ENV='script' npx ts-node server/scripts/seed.ts",
    "cleanupOrphanedTags": "NODE_ENV='script' npx ts-node server/scripts/cleanupOrphanedTags.ts",
    "build:be": "tsc",
    "build:fe": "webpack --mode=production",
    "build": "npm run build:be & npm run build:fe",
    "demo": "node built_server",
    "start": "NODE_ENV='prod' node built_server",
    "dev:be": "ts-node-dev server",
    "dev:fe": "node built_server & webpack serve --mode=development",
    "dev": "ts-node-dev server & webpack serve --mode=development",
    "test:be": "NODE_ENV='test' mocha -r ts-node/register --file server/mochaSetup.ts --timeout 10000 --exit -- 'server/**/__tests__/*.spec.ts'"
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "**.{jsx, jsx}": [
      "npm run lint:fix -- --max-warnings=0"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "cheerio": "^1.0.0-rc.10",
    "class-validator": "^0.13.1",
    "classnames": "^2.2.6",
    "compression": "^1.7.4",
    "connect-typeorm": "^1.1.4",
    "core-js": "^3.16.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.12.1",
    "focus-trap-react": "^8.3.2",
    "helmet": "^4.2.0",
    "normalize.css": "^8.0.1",
    "passport": "^0.4.1",
    "pg": "^8.5.1",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.13.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.9",
    "reselect": "^4.0.0",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/cheerio": "^0.22.30",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.43",
    "@types/passport": "^1.0.6",
    "@types/supertest": "^2.0.11",
    "babel-loader": "^8.2.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "mini-css-extract-plugin": "^2.2.0",
    "mocha": "^9.1.3",
    "node-sass": "^6.0.1",
    "react-refresh": "^0.9.0",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "supertest": "^6.1.6",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5",
    "volleyball": "^1.5.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
