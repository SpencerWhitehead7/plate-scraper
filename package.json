{
  "name": "plate-scraper",
  "version": "1.0.0",
  "description": "An app for collecting, editing, organizing, and storing recipes",
  "author": "Spencer Whitehead",
  "license": "Unlicense",
  "main": "server/index",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpencerWhitehead7/plate-scraper.git"
  },
  "scripts": {
    "lint": "eslint ./server ./client",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf built node_modules && npm install",
    "seed": "NODE_ENV='script' npx ts-node server/scripts/seed.ts",
    "cleanupOrphanedTags": "NODE_ENV='script' npx ts-node server/scripts/cleanupOrphanedTags.ts",
    "build:be": "tsc --project server/tsconfig.json",
    "build:fe": "vite build --emptyOutDir",
    "build": "npm run build:be & npm run build:fe",
    "start": "NODE_ENV='prod' node built/server",
    "dev:be": "ts-node-dev --project server/tsconfig.json server",
    "dev:fe": "vite dev",
    "test:be": "NODE_ENV='test' TS_NODE_PROJECT='./server/tsconfig.json' mocha -r ts-node/register --file server/mochaSetup.ts --timeout 10000 -- 'server/**/__tests__/*.spec.ts'",
    "test:fe": "ECHO not implemented",
    "test": "npm run test:be && npm run test:fe"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "bcrypt": "^5.1.0",
    "cheerio": "^1.0.0-rc.12",
    "class-validator": "0.14.0",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "connect-typeorm": "^2.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "focus-trap-react": "^10.0.2",
    "helmet": "^6.0.1",
    "normalize.css": "^8.0.1",
    "passport": "0.6.0",
    "pg": "^8.8.0",
    "qs": "^6.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.41.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.1",
    "typeorm": "^0.3.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/cheerio": "^0.22.31",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.0.0",
    "@types/passport": "^1.0.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "mocha": "^10.2.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.67.0",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.2",
    "volleyball": "^1.5.1"
  }
}
