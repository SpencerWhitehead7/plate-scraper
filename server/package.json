{
  "name": "plate-scraper-be",
  "version": "1.0.0",
  "description": "Backend for an app for collecting, editing, organizing, and storing recipes",
  "author": "Spencer Whitehead",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpencerWhitehead7/plate-scraper.git"
  },
  "scripts": {
    "lint": "eslint ./src ./scripts",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -rf node_modules ./dist && npm install",
    "dev": "ts-node-dev ./src",
    "test": "NODE_ENV='test' mocha -r ts-node/register --file mochaSetup.ts --timeout 10000 -- '**/__tests__/*.spec.ts'",
    "build": "rm -rf ./dist && tsc",
    "seed": "NODE_ENV='script' npx ts-node scripts/seed.ts",
    "cleanupOrphanedTags": "NODE_ENV='script' npx ts-node scripts/cleanupOrphanedTags.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "class-validator": "0.14.0",
    "compression": "^1.7.4",
    "connect-typeorm": "^2.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "passport": "0.7.0",
    "pg": "^8.11.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/cheerio": "^0.22.35",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.4",
    "@types/passport": "^1.0.16",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "mocha": "^10.2.0",
    "supertest": "^6.3.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "volleyball": "^1.5.1"
  }
}
